    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:headed": "cypress run --headed",
    "test:login": "cypress run --spec 'cypress/features/login.feature'",
    "test:register": "cypress run --spec 'cypress/features/register.feature'",
    "test:all": "cypress run --spec 'cypress/features/**/*.feature'"
